<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_seven_targets_auth_form" model="ir.ui.view">
    <field name="name">seven.targets.auth.form</field>
    <field name="model">seven.targets.auth</field>
    <field name="type">form</field>
    <field name="arch" type="xml">
    <form>
        <sheet>
            <group>
                <field name="client_id"/>
                <field name="client_secret"/>
                <field name="user_identifier"/>
            </group>
        </sheet>
    </form>
    </field>
    </record>

    <record id="view_seven_targets_auth_tree" model="ir.ui.view">
    <field name="name">seven.targets.auth.tree</field>
    <field name="model">seven.targets.auth</field>
    <field name="arch" type="xml">
        <tree>
            <field name="user_identifier"/>
        </tree>
    </field>
    </record>


</odoo>